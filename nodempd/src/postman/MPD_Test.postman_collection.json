									"});\r",
									"\r",
									"pm.test(\"Response has ID\", function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    pm.expect(jsonData).to.have.property(\"id\");\r",
									"});\r",
									"\r",
									"pm.test(\"Response has nombreCorto\", function () {\r",
									"    const jsonData = pm.response.json();\r",
